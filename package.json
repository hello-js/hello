{
  "name": "startkit-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "swc lib -d dist --delete-dir-on-start",
    "build:watch": "swc lib -d dist --watch --quiet",
    "check": "run-s type-check lint format:check test",
    "clean": "rm -rf dist",
    "dev": "run-p build:watch start:dev --silent",
    "format": "prettier  --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts",
    "start": "node dist/index.js",
    "start:dev": "nodemon \"./dist/index.js\" --watch dist --quiet",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.121",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.4.3",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.4.3",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
